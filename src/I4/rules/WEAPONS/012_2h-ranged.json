{
	"$schema": "https://raw.githubusercontent.com/GroundAura/InventoryInjector/main/docs/InventoryInjector.schema.json",
	"rules": [
		{
			"match": {
				"formType": "Weapon",
				"keywords": "OCF_WeapTypeBow2H"
			},
			"assign": {
				"itemClass": "3",
				"itemSort": "2",
				"subTypeDisplay": "$Bow",
				"iconLabel": "weapon_bow"
			}
		},
		{
			"match": {
				"formType": "Weapon",
				"keywords": "OCF_WeapTypeBowblade2H"
			},
