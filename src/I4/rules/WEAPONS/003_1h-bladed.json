				"subTypeDisplay": "$ItemWeaponRapier",
				"iconSource": "BOOBIES Immersive Icons/weapons.swf",
				"iconLabel": "weapon_rapier"
			}
		},
		{
			"match": {
				"formType": "Weapon",
				"keywords": "OCF_WeapTypeSaber1H"
			},
			"assign": {
				"itemClass": "1",
				"itemSort": "4",
				"subTypeDisplay": "$ItemWeaponSaber",
				"iconSource": "AIT/GameIcons/icons.swf",
				"iconLabel": "weapon_saber"
			}
		},
		{
			"match": {
				"formType": "Weapon",
				"keywords": "OCF_WeapTypeScimitar1H"
			},
			"assign": {
				"itemClass": "1",
				"itemSort": "4",
				"subTypeDisplay": "$ItemWeaponScimitar",
				"iconSource": "AIT/GameIcons/icons.swf",
				"iconLabel": "weapon_saber"
