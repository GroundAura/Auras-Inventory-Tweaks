{
	"$schema": "https://raw.githubusercontent.com/GroundAura/InventoryInjector/main/docs/InventoryInjector.schema.json",
	"rules": [
		{
			"match": {
				"formType": "Weapon",
				"keywords": "OCF_InvColorBlood"
			},
			"assign": {
				"iconColor": "ColorWeaponThrownBlood"
			}
		},
		{
			"match": {
				"formType": "Weapon",
				"keywords": "OCF_InvColorDwarven"
			},
			"assign": {
				"iconColor": "ColorStaffDwarven"
			}
		},
		{
			"match": {
				"formType": "Weapon",
				"keywords": "OCF_InvColorFire"
			},
			"assign": {
				"iconColor": "ColorWeaponThrownFire"
			}
		},
		{
			"match": {
				"formType": "Weapon",
				"keywords": "OCF_InvColorFrost"
			},
			"assign": {
				"iconColor": "ColorWeaponThrownFrost"
			}
		},
		{
			"match": {
				"formType": "Weapon",
				"keywords": "OCF_InvColorPoison"
			},
			"assign": {
				"iconColor": "ColorWeaponThrownPoision"
			}
		},
		{
			"match": {
				"formType": "Weapon",
				"keywords": "OCF_InvColorShock"
			},
			"assign": {
				"iconColor": "ColorWeaponThrownShock"
			}
		},
		{
			"match": {
				"formType": "Weapon",
				"keywords": "OCF_InvColorSun"
			},
			"assign": {
				"iconColor": "ColorWeaponThrownSun"
			}
		},
		{
			"match": {
				"formType": "Weapon",
				"keywords": "OCF_InvColorWater"
			},
			"assign": {
				"iconColor": "ColorWeaponThrownWater"
			}
		}
	]
}