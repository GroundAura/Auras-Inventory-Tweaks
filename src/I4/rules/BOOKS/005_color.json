{
	"$schema": "https://raw.githubusercontent.com/GroundAura/InventoryInjector/main/docs/InventoryInjector.schema.json",
	"rules": [
		{
			"match": {
				"formType": "Book",
				"keywords": [
					"OCF_InvColorAedric"
				]
			},
			"assign": {
				"iconColor": "ColorRarityAedric"
			}
		},
		{
			"match": {
				"formType": "Book",
				"keywords": [
					"OCF_InvColorDaedric"
				]
			},
			"assign": {
				"iconColor": "ColorRarityDaedric"
			}
		},
		{
			"match": {
				"formType": "Book",
				"keywords": [
					"OCF_InvColorEldritch"
				]
			},
			"assign": {
				"iconColor": "ColorSpellEldritch"
			}
		}
	]
}