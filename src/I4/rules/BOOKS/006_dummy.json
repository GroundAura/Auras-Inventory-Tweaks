{
	"$schema": "https://raw.githubusercontent.com/GroundAura/InventoryInjector/main/docs/InventoryInjector.schema.json",
	"rules": [
		{
			"match": {
				"formType": "Book",
				"keywords": [
					"CraftingSmithingForge"
				]
			},
			"assign": {
				"itemClass": "0"
			}
		},
		{
			"match": {
				"formType": "Book",
				"keywords": [
					"OCF_Placeholder_Craft"
				]
			},
			"assign": {
				"itemType": "0",
				"itemClass": "0",
				"itemSort": "0",
				"subTypeDisplay": "$ItemPlaceholderCraft",
				"iconSource": "I4IconAddon/icons.swf",
				"iconLabel": "misc_tool"
			}
		},
		{
			"match": {
				"formType": "Book",
				"keywords": [
					"OCF_Placeholder_FireFuel"
				]
			},
			"assign": {
				"itemType": "0",
				"itemClass": "0",
				"itemSort": "0",
				"subTypeDisplay": "$ItemPlaceholderAddFuel"
			}
		},
		{
			"match": {
				"formType": "Book",
				"keywords": [
					"OCF_Placeholder_FireFuel_Beehive"
				]
			},
			"assign": {
				"iconSource": "AIT/GameIcons/icons.swf",
				"iconLabel": "misc_herbs"
			}
		},
		{
			"match": {
				"formType": "Book",
				"keywords": [
					"OCF_Placeholder_FireFuel_Book"
				]
			},
			"assign": {
				"iconLabel": "default_book"
			}
		},
		{
			"match": {
				"formType": "Book",
				"keywords": [
					"OCF_Placeholder_FireFuel_Cloth"
				]
			},
			"assign": {
				"iconSource": "AIT/GameIcons/icons.swf",
				"iconLabel": "misc_cloth"
			}
		},
		{
			"match": {
				"formType": "Book",
				"keywords": [
					"OCF_Placeholder_FireFuel_DwarvenOil"
				]
			},
			"assign": {
				"iconSource": "AIT/MoreIcons/icons.swf",
				"iconLabel": "food_stew",
				"iconColor": "ColorMiscMetalDwarven"
			}
		},
		{
			"match": {
				"formType": "Book",
				"keywords": [
					"OCF_Placeholder_FireFuel_Fat"
				]
			},
			"assign": {
				"iconSource": "AIT/MoreIcons/icons.swf",
				"iconLabel": "food_stew"
			}
		},
		{
			"match": {
				"formType": "Book",
				"keywords": [
					"OCF_Placeholder_FireFuel_FireSalts"
				]
			},
			"assign": {
				"iconSource": "AIT/MoreIcons/icons.swf",
				"iconLabel": "food_stew",
				"iconColor": "ColorIngrFire"
			}
		},
		{
			"match": {
				"formType": "Book",
				"keywords": [
					"OCF_Placeholder_FireFuel_LanternOil"
				]
			},
			"assign": {
				"iconLabel": "default_potion"
			}
		},
		{
			"match": {
				"formType": "Book",
				"keywords": [
					"OCF_Placeholder_FireFuel_Mushroom"
				]
			},
			"assign": {
				"iconSource": "AIT/GameIcons/icons.swf",
				"iconLabel": "food_mushroom",
				"iconColor": "ColorIngrMushroom"
			}
		},
		{
			"match": {
				"formType": "Book",
				"keywords": {
					"anyOf": [
						"OCF_Placeholder_FireFuel_Paper",
						"OCF_Placeholder_FireFuel_Straw"
					]
				}
			},
			"assign": {
				"iconLabel": "misc_clutter"
			}
		},
		{
			"match": {
				"formType": "Book",
				"keywords": [
					"OCF_Placeholder_FireFuel_Plant"
				]
			},
			"assign": {
				"iconSource": "AIT/GameIcons/icons.swf",
				"iconLabel": "misc_herbs",
				"iconColor": "ColorIngrPlant"
			}
		},
		{
			"match": {
				"formType": "Book",
				"keywords": [
					"OCF_Placeholder_FireFuel_Root"
				]
			},
			"assign": {
				"iconSource": "AIT/GameIcons/icons.swf",
				"iconLabel": "misc_herbs",
				"iconColor": "ColorIngrRoot"
			}
		},
		{
			"match": {
				"formType": "Book",
				"keywords": [
					"OCF_Placeholder_FireFuel_Wood"
				]
			},
			"assign": {
				"iconLabel": "misc_wood",
				"iconColor": "ColorMiscWood"
			}
		},
		{
			"match": {
				"formType": "Book",
				"keywords": [
					"OCF_Placeholder_FireLight"
				]
			},
			"assign": {
				"itemType": "0",
				"itemClass": "0",
				"itemSort": "0",
				"subTypeDisplay": "$ItemPlaceholderLightFire"
			}
		},
		{
			"match": {
				"formType": "Book",
				"keywords": [
					"OCF_Placeholder_FireLight_Magic"
				]
			},
			"assign": {
				"iconLabel": "magic_fire",
				"iconColor": "ColorSpellFireDefault"
			}
		},
		{
			"match": {
				"formType": "Book",
				"keywords": [
					"OCF_Placeholder_FireLight_Stone"
				]
			},
			"assign": {
				"iconSource": "I4IconAddon/icons.swf",
				"iconLabel": "magic_earth",
				"iconColor": "ColorMiscStone"
			}
		}
	]
}