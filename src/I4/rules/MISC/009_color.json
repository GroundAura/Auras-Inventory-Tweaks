{
	"$schema": "https://raw.githubusercontent.com/GroundAura/InventoryInjector/main/docs/InventoryInjector.schema.json",
	"rules": [
		{
			"match": {
				"formType": "MiscItem",
				"keywords": {
					"anyOf": [
						"OCF_InvColorCopper",
						"OCF_MiscTableware_Bronze",
						"OCF_MiscTreasure_Bronze"
					]
				}
			},
			"assign": {
				"iconColor": "ColorMiscCopper"
			}
		},
		{
			"match": {
				"formType": "MiscItem",
				"keywords": {
					"anyOf": [
						"OCF_InvColorGold",
						"OCF_MiscTableware_Gold",
						"OCF_MiscTreasure_Gold"
					]
				}
			},
			"assign": {
				"iconColor": "ColorMiscGold"
			}
		},
		{
			"match": {
				"formType": "MiscItem",
				"keywords": {
					"anyOf": [
						"OCF_InvColorSilver",
						"OCF_MiscTableware_Silver",
						"OCF_MiscTreasure_Silver"
					]
				}
			},
			"assign": {
				"iconColor": "ColorMiscSilver"
			}
		},
		{
			"match": {
				"formType": "MiscItem",
				"keywords": [
					"OCF_InvColorDaedric"
				]
			},
			"assign": {
				"iconColor": "ColorMiscDaedric"
			}
		},
		{
			"match": {
				"formType": "MiscItem",
				"keywords": [
					"OCF_InvColorEldritch"
				]
			},
			"assign": {
				"iconColor": "ColorMiscEldritch"
			}
		},
		{
			"match": {
				"formType": "MiscItem",
				"keywords": [
					"OCF_InvColorLegendary"
				]
			},
			"assign": {
				"iconColor": "ColorRarityLegendary"
			}
		},
		{
			"match": {
				"formType": "MiscItem",
				"keywords": [
					"OCF_InvColorNecromancy"
				]
			},
			"assign": {
				"iconColor": "ColorMiscNecromancy"
			}
		},
		{
			"match": {
				"formType": "MiscItem",
				"keywords": [
					"OCF_InvColorRed"
				]
			},
			"assign": {
				"iconColor": "ColorMiscRed"
			}
		}
	]
}