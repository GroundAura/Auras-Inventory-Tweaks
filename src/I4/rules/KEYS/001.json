{
	"$schema": "https://raw.githubusercontent.com/GroundAura/InventoryInjector/main/docs/InventoryInjector.schema.json",
	"rules": [
		{
			"match": {
				"formType": "Key",
				"keywords": [
					"OCF_KeyPlayerHome"
				]
			},
			"assign": {
				"subTypeDisplay": "$ItemKeyHome",
				"iconLabel": "key_house"
			}
		},
		{
			"match": {
				"formType": "Key",
				"keywords": [
					"OCF_IngrRemains_BoneSkull"
				]
			},
			"assign": {
				"subTypeDisplay": "$Remains",
				"iconLabel": "misc_remains"
			}
		},
		{
			"match": {
				"formType": "Key",
				"keywords": [
					"OCF_RelicDaedric"
				]
			},
			"assign": {
				"subTypeDisplay": "$ItemMiscRelic",
				"iconSource": "AIT/Aura/icons.swf",
				"iconLabel": "misc_artifactdaedra",
				"iconColor": "ColorRarityDaedric"
			}
		},
		{
			"match": {
				"formType": "Key",
				"keywords": [
					"OCF_RelicDwarven"
				]
			},
			"assign": {
				"subTypeDisplay": "$ItemMiscRelic",
				"iconSource": "I4IconAddon/icons.swf",
				"iconLabel": "misc_dwarvenscrap",
				"iconColor": "ColorMiscMetalDwarven"
			}
		}
	]
}