{
	"$schema": "https://raw.githubusercontent.com/GroundAura/InventoryInjector/main/docs/InventoryInjector.schema.json",
	"rules": [
		{
			"match": {
				"formType": "Ingredient"
			},
			"assign": {
				"itemType": "6",
				"foodGreenPact": "0",
				"iconLabel": "default_ingredient"
			}
		},
		{
			"match": {
				"formType": "Ingredient",
				"keywords": [
					"OCF_AlchGreenPact"
				]
			},
			"assign": {
				"foodGreenPact": "1",
				"foodGreenPactDisplay": "$GreenPactTrue"
			}
		}
	]
}