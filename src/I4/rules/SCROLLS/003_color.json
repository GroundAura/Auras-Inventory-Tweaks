{
	"$schema": "https://raw.githubusercontent.com/GroundAura/InventoryInjector/main/docs/InventoryInjector.schema.json",
	"rules": [
		{
			"match": {
				"formType": "Scroll",
				"keywords": [
					"OCF_InvColorFire"
				]
			},
			"assign": {
				"itemSort": "1",
				"iconColor": "ColorScrollFire"
			}
		},
		{
			"match": {
				"formType": "Scroll",
				"keywords": [
					"OCF_InvColorFrost"
				]
			},
			"assign": {
				"itemSort": "2",
				"iconColor": "ColorScrollFrost"
			}
		},
		{
			"match": {
				"formType": "Scroll",
				"keywords": [
					"OCF_InvColorShock"
				]
			},
			"assign": {
				"itemSort": "3",
				"iconColor": "ColorScrollShock"
			}
		},
		{
			"match": {
				"formType": "Scroll",
				"keywords": [
					"OCF_InvColorPoison"
				]
			},
			"assign": {
				"itemSort": "4",
				"iconColor": "ColorScrollPoison"
			}
		}
	]
}