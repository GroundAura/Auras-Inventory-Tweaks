{
	"$schema": "https://raw.githubusercontent.com/GroundAura/InventoryInjector/main/docs/InventoryInjector.schema.json",
	"rules": [
		{
			"match": {
				"formType": "SoulGem",
				"keywords": {
					"anyOf": [
						"OCF_RelicAyleid_Crystal",
						"IconAyleidCrystal"
					]
				},
				"status": "Empty"
			},
			"assign": {
				"subTypeDisplay": "$ItemMiscAyleidCrystal",
				"iconSource": "I4IconAddon/icons.swf",
				"iconLabel": "soulgem_ayleidcrystalempty",
				"iconColor": "ColorSlgmAyleid"
			}
		},
		{
			"match": {
				"formType": "SoulGem",
				"keywords": {
					"anyOf": [
						"OCF_RelicAyleid_Crystal",
						"IconAyleidCrystal"
					]
				},
				"status": "Partial"
			},
			"assign": {
				"subTypeDisplay": "$ItemMiscAyleidCrystal",
				"iconSource": "I4IconAddon/icons.swf",
				"iconLabel": "soulgem_ayleidcrystalpartial",
				"iconColor": "ColorSlgmAyleid"
			}
		},
		{
			"match": {
				"formType": "SoulGem",
				"keywords": {
					"anyOf": [
						"OCF_RelicAyleid_Crystal",
						"IconAyleidCrystal"
					]
				},
				"status": "Full"
			},
			"assign": {
				"subTypeDisplay": "$ItemMiscAyleidCrystal",
				"iconSource": "I4IconAddon/icons.swf",
				"iconLabel": "soulgem_ayleidcrystalfull",
				"iconColor": "ColorSlgmAyleid"
			}
		},
		{
			"match": {
				"formType": "SoulGem",
				"keywords": [
					"OCF_RelicDaedric_SigilStone"
				]
			},
			"assign": {
				"subTypeDisplay": "$ItemMiscSigilStone",
				"iconSource": "AIT/Aura/icons.swf",
				"iconLabel": "misc_artifactdaedra",
				"iconColor": "ColorSlgmSigilStone"
			}
		},
		{
			"match": {
				"formType": "SoulGem",
				"keywords": [
					"OCF_RelicDaedric_SoulTomato"
				],
				"status": "Empty"
			},
			"assign": {
				"subTypeDisplay": "$ItemMiscSoulTomato",
				"iconSource": "I4IconAddon/icons.swf",
				"iconLabel": "soulgem_tomatoempty",
				"iconColor": "ColorSlgmTomato"
			}
		},
		{
			"match": {
				"formType": "SoulGem",
				"keywords": [
					"OCF_RelicDaedric_SoulTomato"
				],
				"status": "Partial"
			},
			"assign": {
				"subTypeDisplay": "$ItemMiscSoulTomato",
				"iconSource": "I4IconAddon/icons.swf",
				"iconLabel": "soulgem_tomatopartial",
				"iconColor": "ColorSlgmTomato"
			}
		},
		{
			"match": {
				"formType": "SoulGem",
				"keywords": [
					"OCF_RelicDaedric_SoulTomato"
				],
				"status": "Full"
			},
			"assign": {
				"subTypeDisplay": "$ItemMiscSoulTomato",
				"iconSource": "I4IconAddon/icons.swf",
				"iconLabel": "soulgem_tomatofull",
				"iconColor": "ColorSlgmTomato"
			}
		}
	]
}