{
	"$schema": "https://raw.githubusercontent.com/GroundAura/InventoryInjector/main/docs/InventoryInjector.schema.json",
	"rules": [
		{
			"match": {
				"formType": "Potion",
				"keywords": [
					"OCF_InvColorDaedric"
				]
			},
			"assign": {
				"iconColor": "ColorRarityDaedric"
			}
		},
		{
			"match": {
				"formType": "Potion",
				"keywords": [
					"OCF_InvColorLegendary"
				]
			},
			"assign": {
				"iconColor": "ColorRarityLegendary"
			}
		}
	]
}