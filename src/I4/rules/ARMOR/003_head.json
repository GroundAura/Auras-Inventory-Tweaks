{
	"$schema": "https://raw.githubusercontent.com/GroundAura/InventoryInjector/main/docs/InventoryInjector.schema.json",
	"rules": [
		{
			"match": {
				"formType": "Armor",
				"keywords": [
					"OCF_HeadTypeBandage"
				]
			},
			"assign": {
				"subTypeDisplay": "$ItemArmorFace",
				"iconSource": "AIT/MoreIcons/icons.swf",
				"iconLabel": "armor_eyepatch"
			}
		},
		{
			"match": {
				"formType": "Armor",
				"keywords": [
					"OCF_HeadTypeBandana"
				]
			},
			"assign": {
				"subTypeDisplay": "$ItemArmorBandana",
				"iconSource": "AIT/MoreIcons/icons.swf",
				"iconLabel": "armor_bandana"
			}
		},
