			"match": {
				"formType": "Armor",
				"keywords": {
					"anyOf": [
						"OCF_BodyTypeGorget",
						"ClothingChoker"
					]
				}
			},
			"assign": {
				"subTypeDisplay": "$ItemArmorChoker",
				"iconSource": "BOOBIES Immersive Icons/bodyparts.swf",
				"iconLabel": "armor_choker"
			}
		},
		{
			"match": {
				"formType": "Armor",
