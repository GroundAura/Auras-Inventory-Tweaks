{
	"$schema": "https://raw.githubusercontent.com/GroundAura/InventoryInjector/main/docs/InventoryInjector.schema.json",
	"rules": [
		{
			"match": {
				"formType": "Spell",
				"spellType": {
					"anyOf": [
						"Spell",
						"Power",
						"LesserPower"
					]
				},
				"school": "Alteration",
				"effectKeywords": {
					"anyOf": [
						"OCF_MgefSpellDispel",
						"MAG_MagicDispelSpell"
					]
				}
			},
			"assign": {
				"magicClassDisplay": "$MagicArcane",
				"subTypeDisplay": "$MagicDispel",
				"iconLabel": "default_alteration",
				"iconColor": "ColorSpellDispelDefault"
			}
		},
		{
			"match": {
				"formType": "Spell",
				"spellType": {
					"anyOf": [
						"Spell",
						"Power",
						"LesserPower"
					]
				},
				"school": "Conjuration",
				"effectKeywords": {
					"anyOf": [
						"OCF_MgefSpellDispel",
						"MAG_MagicDispelSpell"
					]
				}
			},
			"assign": {
				"magicClassDisplay": "$MagicArcane",
				"subTypeDisplay": "$MagicDispel",
				"iconLabel": "default_conjuration",
				"iconColor": "ColorSpellDispelSummon"
			}
		},
		{
			"match": {
				"formType": "Spell",
				"spellType": {
					"anyOf": [
						"Spell",
						"Power",
						"LesserPower"
					]
				},
				"school": "Illusion",
				"effectKeywords": {
					"anyOf": [
						"OCF_MgefSpellDispel",
						"MAG_MagicDispelSpell"
					]
				}
			},
			"assign": {
				"magicClassDisplay": "$MagicArcane",
				"subTypeDisplay": "$MagicDispel",
				"iconLabel": "default_illusion",
				"iconColor": "ColorSpellDispelDefault"
			}
		},
		{
			"match": {
				"formType": "Spell",
				"spellType": {
					"anyOf": [
						"Spell",
						"Power",
						"LesserPower"
					]
				},
				"school": "Restoration",
				"effectKeywords": {
					"anyOf": [
						"OCF_MgefSpellDispel",
						"MAG_MagicDispelSpell"
					]
				}
			},
			"assign": {
				"magicClassDisplay": "$MagicArcane",
				"subTypeDisplay": "$MagicDispel",
				"iconLabel": "default_restoration",
				"iconColor": "ColorSpellDispelDefault"
			}
		}
	]
}