{
"$schema": "https://raw.githubusercontent.com/GroundAura/InventoryInjector/main/docs/InventoryInjector.schema.json",
"rules": [
	{
		"match": {
			"formType": "Potion"
		},
		"assign": {
			"sortByEffectData": "1"
		}
	},
	{
		"match": {
			"formType": "Potion",
			"keywords": { "anyOf": [ "VendorItemPotion", "VendorItemPoison" ] }
		},
		"assign": {
			"itemType": "3",
			"itemClass": "6"
		}
	},
	{
		"match": {
			"formType": "Potion",
			"subType": { "anyOf": [ "Health", "HealRate", "HealRateMult" ] }
		},
		"assign": {
			"itemClass": "1"
		}
	},
	{
		"match": {
			"formType": "Potion",
			"subType": { "anyOf": [ "Stamina", "StaminaRate", "StaminaRateMult" ] }
		},
		"assign": {
			"itemClass": "2"
		}
	},
	{
		"match": {
			"formType": "Potion",
			"subType": { "anyOf": [ "Magicka", "MagickaRate", "MagickaRateMult" ] }
		},
		"assign": {
			"itemClass": "3"
		}
	},
	{
		"match": {
			"formType": "Potion",
			"subType": "Potion",
			"archetype": {
				"anyOf": [
					"CureAddiction",
					"CureDisease",
					"CureParalysis",
					"CurePoison"
				]
			}
		},
		"assign": {
			"itemClass": "4",
			"subTypeDisplay": "$MagicCure",
			"iconColor": "#DB2E73"
		}
	},
	{
		"match": {
			"formType": "Potion",
			"subType": "Potion",
			"archetype": "PeakValueModifier"
		},
		"assign": {
			"itemClass": "7",
			"subTypeDisplay": "$MagicFortify"
		}
	},
	{
		"match": {
			"formType": "Potion",
			"subType": "Potion",
			"archetype": "PeakValueModifier",
			"actorValue": "CarryWeight"
		},
		"assign": {
			"subTypeDisplay": "$MagicEnhance",
			"iconColor": "#51DB2E"
		}
